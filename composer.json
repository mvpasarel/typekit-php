{
    "name": "mvpasarel/typekit-php",
    "description": "PHP wrapper for Typekit Developer API",
    "keywords": [
        "typekit"
    ],
    "homepage": "https://github.com/mvpasarel/typekit-php",
    "license": "MIT",
    "authors": [
        {
            "name": "Madalin Pasarel",
            "email": "madalin.pasarel@gmail.com",
            "homepage": "http://www.mvpasarel.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0",
        "guzzle/guzzle": "~3.9"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Mvpasarel\\Typekit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvpasarel\\Typekit\\Test\\": "tests"
        }
    }
}
